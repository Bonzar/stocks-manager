{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "node16",
    "module": "ES2022",
    "target": "ES2022",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noStrictGenericChecks": false,
    "noEmitOnError": false,
  },
  "include": ["src/**/*.ts"]
}
